<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.geekjoker.joy.blog.mapper.NoteUpdateMapper" >
    <update id="noteUpdate" parameterType="note">
        update note set
        update_time=#{updateTime},
        title=#{title},
        content=#{content},
        favorite=#{favorite},
        wasted=#{wasted}
        where id=#{id}
    </update>
</mapper>