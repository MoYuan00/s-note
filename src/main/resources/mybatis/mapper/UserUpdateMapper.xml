<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.geekjoker.joy.blog.mapper.UserUpdateMapper" >
    <update id="userUpdatePassWord" parameterType="user">
        update user set update_time=#{updateTime}, password=#{password}
        where id=#{id}
    </update>
</mapper>