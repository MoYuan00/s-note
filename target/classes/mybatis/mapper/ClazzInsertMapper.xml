<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.geekjoker.joy.blog.mapper.ClazzInsertMapper" >
    <insert useGeneratedKeys="true"
            keyColumn="id"
            keyProperty="id"
            id="clazzInsert" parameterType="clazz">
        insert into class
        (create_time, update_time, title, detail, user_id) values
        ( #{createTime}, #{updateTime}, #{title}, #{detail}, #{userId})
    </insert>
</mapper>