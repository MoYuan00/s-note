<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.geekjoker.joy.blog.mapper.ClazzUpdateMapper" >
    <update id="clazzUpdate" parameterType="clazz">
        update class set
        update_time=#{updateTime},
        title=#{title},
        detail=#{detail}
        where id=#{id}
    </update>
</mapper>